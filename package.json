{
    "name": "vyaktitva",
    "description": "( व्यक्तित्व ) — A component based UI library for Vasanth Developer's projects.",
    "version": "0.2.4",
    "license": "zlib",
    "type": "module",
    "types": "dist/index.d.ts",
    "main": "dist/vyaktitva.es.js",
    "homepage": "https://github.com/vsnthdev/vyaktitva#readme",
    "bugs": "https://github.com/vsnthdev/vyaktitva/issues",
    "author": {
        "name": "Vasanth Developer",
        "email": "vasanth@vasanthdeveloper.com",
        "url": "https://vsnth.dev"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/vsnthdev/vyaktitva.git"
    },
    "engines": {
        "node": "16.x"
    },
    "files": [
        "dist",
        "tailwind.config.cjs"
    ],
    "scripts": {
        "dev": "vite --config vite.config.ts src",
        "clean": "rimraf dist",
        "build": "rimraf dist && tsc \"src/lib/index.ts\" --strict --allowJs false --outDir dist -t esnext --jsx react-jsx -d --emitDeclarationOnly --skipLibCheck true --isolatedModules true --esModuleInterop false --resolveJsonModule true --moduleResolution node --useDefineForClassFields true --allowSyntheticDefaultImports true --forceConsistentCasingInFileNames true --lib \"DOM, DOM.Iterable, ESNext\" && vite build --config vite.config.ts src"
    },
    "devDependencies": {
        "@types/debounce": "^1.2.1",
        "@types/node": "^18.11.18",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.10",
        "@vitejs/plugin-react": "^3.0.0",
        "autoprefixer": "^10.4.13",
        "postcss": "^8.4.20",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.9.4",
        "vite": "^4.0.3"
    },
    "peerDependencies": {
        "tailwindcss": "^3.2.4"
    },
    "dependencies": {
        "debounce": "^1.2.1",
        "react-svg-blob": "^0.3.1"
    }
}