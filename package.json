        "name": "Vasanth Developer"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/vsnthdev/vyaktitva.git"
    },
    "engines": {
        "node": ">=18"
    },
    "files": [
        "dist",
        "tailwind.config.cjs"
    ],
    "scripts": {
        "dev": "vite --config vite.config.ts src",
        "clean": "rimraf dist",
        "build": "rimraf dist && tsc \"src/lib/index.ts\" --strict --allowJs false --outDir dist -t esnext --jsx react-jsx -d --emitDeclarationOnly --skipLibCheck true --isolatedModules true --esModuleInterop false --resolveJsonModule true --moduleResolution node --useDefineForClassFields true --allowSyntheticDefaultImports true --forceConsistentCasingInFileNames true --lib \"DOM, DOM.Iterable, ESNext\" && vite build --config vite.config.ts src"
    },
    "devDependencies": {
        "@types/debounce": "^1.2.1",
        "@types/node": "^18.14.4",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@vitejs/plugin-react": "^3.1.0",
        "autoprefixer": "^10.4.13",
        "postcss": "^8.4.21",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^4.1.3",
        "typescript": "^4.9.5",
        "vite": "^4.1.4"
    },
    "peerDependencies": {
        "tailwindcss": "^3.2.7"
    },
    "dependencies": {
        "debounce": "^1.2.1",
        "kbar": "^0.1.0-beta.40",
        "react-svg-blob": "^0.3.1"
    }
